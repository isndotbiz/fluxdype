{
  "workflow_info": {
    "name": "Ultra Realistic Batch Generator",
    "description": "High-quality batch image generation optimized for phone apps",
    "version": "1.0",
    "resolution": "1024x1536 (portrait) or 1536x1024 (landscape)",
    "optimized_for": "phone_app_content",
    "quality_level": "ultra_high"
  },
  "prompt": {
    "1": {
      "inputs": {
        "ckpt_name": "flux1-dev-fp8.safetensors"
      },
      "class_type": "CheckpointLoaderSimple",
      "_meta": {
        "title": "Load Flux Kria FP8 Model"
      }
    },
    "2": {
      "inputs": {
        "text": "ultra realistic, high resolution, 8k uhd, professional photography, detailed, sharp focus, perfect composition, masterpiece, best quality",
        "clip": ["1", 1]
      },
      "class_type": "CLIPTextEncode",
      "_meta": {
        "title": "Positive Prompt"
      }
    },
    "3": {
      "inputs": {
        "text": "low quality, blurry, pixelated, compressed, jpeg artifacts, watermark, text, logo, oversaturated, cartoon, anime, painting, illustration, unrealistic, distorted, deformed, amateur, grainy",
        "clip": ["1", 1]
      },
      "class_type": "CLIPTextEncode",
      "_meta": {
        "title": "Negative Prompt"
      }
    },
    "4": {
      "inputs": {
        "seed": 0,
        "steps": 25,
        "cfg": 3.5,
        "sampler_name": "euler",
        "scheduler": "simple",
        "denoise": 1.0,
        "model": ["10", 0],
        "positive": ["2", 0],
        "negative": ["3", 0],
        "latent_image": ["5", 0]
      },
      "class_type": "KSampler",
      "_meta": {
        "title": "Main Sampler - High Quality"
      }
    },
    "5": {
      "inputs": {
        "width": 1024,
        "height": 1536,
        "batch_size": 1
      },
      "class_type": "EmptyLatentImage",
      "_meta": {
        "title": "Image Size - Portrait (9:16)"
      }
    },
    "6": {
      "inputs": {
        "samples": ["4", 0],
        "vae": ["1", 2]
      },
      "class_type": "VAEDecode",
      "_meta": {
        "title": "VAE Decode"
      }
    },
    "7": {
      "inputs": {
        "filename_prefix": "ultra_realistic_batch",
        "images": ["6", 0]
      },
      "class_type": "SaveImage",
      "_meta": {
        "title": "Save Image"
      }
    },
    "10": {
      "inputs": {
        "lora_name": "ultrafluxV1.aWjp.safetensors",
        "strength_model": 0.85,
        "strength_clip": 0.85,
        "model": ["11", 0],
        "clip": ["1", 1]
      },
      "class_type": "LoraLoader",
      "_meta": {
        "title": "LoRA 1: Quality Enhancement"
      }
    },
    "11": {
      "inputs": {
        "lora_name": "facebookQuality.3t4R.safetensors",
        "strength_model": 0.65,
        "strength_clip": 0.65,
        "model": ["1", 0],
        "clip": ["1", 1]
      },
      "class_type": "LoraLoader",
      "_meta": {
        "title": "LoRA 2: Social Media Quality"
      }
    }
  }
}
