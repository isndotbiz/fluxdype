{
  "workflow_info": {
    "name": "Qwen Image Basic Workflow",
    "description": "Basic workflow for Qwen image model using UnetLoaderGGUF",
    "version": "1.0",
    "resolution": "1024x1024",
    "model": "qwen-image-Q3_K_S.gguf",
    "optimized_for": "qwen_image_generation"
  },
  "prompt": {
    "1": {
      "inputs": {
        "unet_name": "qwen-image-Q3_K_S.gguf"
      },
      "class_type": "UnetLoaderGGUF",
      "_meta": {
        "title": "Load Qwen Image Model (GGUF)"
      }
    },
    "2": {
      "inputs": {
        "clip_name1": "clip_l.safetensors",
        "clip_name2": "t5xxl_fp16.safetensors",
        "type": "flux"
      },
      "class_type": "DualClipLoader",
      "_meta": {
        "title": "Load Text Encoders"
      }
    },
    "3": {
      "inputs": {
        "vae_name": "qwen_image_vae.safetensors"
      },
      "class_type": "VAELoader",
      "_meta": {
        "title": "Load VAE"
      }
    },
    "4": {
      "inputs": {
        "text": "a beautiful landscape with mountains, clear blue sky, peaceful atmosphere, highly detailed, professional artwork, masterpiece quality"
      },
      "class_type": "CLIPTextEncode",
      "_meta": {
        "title": "Positive Prompt"
      }
    },
    "5": {
      "inputs": {
        "text": "low quality, blurry, distorted, deformed, ugly, bad composition, watermark, text, logo, unprofessional"
      },
      "class_type": "CLIPTextEncode",
      "_meta": {
        "title": "Negative Prompt"
      }
    },
    "6": {
      "inputs": {
        "width": 1024,
        "height": 1024,
        "batch_size": 1
      },
      "class_type": "EmptyLatentImage",
      "_meta": {
        "title": "Empty Latent Image (1024x1024)"
      }
    },
    "7": {
      "inputs": {
        "seed": 0,
        "steps": 20,
        "cfg": 3.5,
        "sampler_name": "euler",
        "scheduler": "simple",
        "denoise": 1.0,
        "model": ["1", 0],
        "positive": ["4", 0],
        "negative": ["5", 0],
        "latent_image": ["6", 0]
      },
      "class_type": "KSampler",
      "_meta": {
        "title": "KSampler - Main Generation"
      }
    },
    "8": {
      "inputs": {
        "samples": ["7", 0],
        "vae": ["3", 0]
      },
      "class_type": "VAEDecode",
      "_meta": {
        "title": "VAE Decode"
      }
    },
    "9": {
      "inputs": {
        "filename_prefix": "qwen_basic_output",
        "images": ["8", 0]
      },
      "class_type": "SaveImage",
      "_meta": {
        "title": "Save Image"
      }
    }
  }
}
