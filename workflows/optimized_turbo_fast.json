    "inputs": {
      "seed": 0,
      "steps": 8,
      "cfg": 2.0,
      "sampler_name": "euler",
      "scheduler": "simple",
      "denoise": 1.0,
      "model": ["6", 0],
      "positive": ["4", 0],
      "negative": ["5", 0],
      "latent_image": ["8", 0]
    },
    "class_type": "KSampler"
  },
  "10": {
    "inputs": {
      "samples": ["9", 0],
      "vae": ["3", 0]
    },
    "class_type": "VAEDecode"
  },
  "11": {
    "inputs": {
      "filename_prefix": "optimized_turbo_",
