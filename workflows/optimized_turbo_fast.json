  "3": {
    "inputs": {
      "vae_name": "ae.safetensors"
    },
    "class_type": "VAELoader"
  },
  "4": {
    "inputs": {
      "text": "professional photography, studio lighting, sharp focus, best quality, masterpiece",
      "clip": ["2", 0]
    },
    "class_type": "CLIPTextEncode"
  },
  "5": {
    "inputs": {
      "text": "low quality, blurry, distorted, cartoon, anime, painting, 3d, watermark, text",
      "clip": ["2", 0]
