{
  "workflow_info": {
    "name": "Multi-LoRA Turbo Optimizer",
    "description": "Optimized Flux Turbo with stacked LoRAs for best quality + speed tradeoff (8-10 seconds)",
    "version": "2.0",
    "resolution": "1024x1024",
    "optimized_for": "quality_turbo_balanced",
    "quality_level": "high_quality_fast",
    "inference_time_seconds": "8-10",
    "steps": 10,
    "cfg": 2.2,
    "lora_stack": [
      {
        "lora": "FLUX.1-Turbo-Alpha.safetensors",
        "strength": 0.7,
        "strengthTwo": 0.7,
        "on": true
      },
      {
        "lora": "facebookQuality.3t4R.safetensors",
        "strength": 0.6,
        "strengthTwo": 0.6,
        "on": true
      },
      {
        "lora": "DetailTweaker_SDXL.safetensors",
        "strength": 0.5,
        "strengthTwo": 0.5,
        "on": true
      }
    ]
  },
  "prompt": {
    "1": {
      "inputs": {
        "unet_name": "flux1-krea-dev_fp8_scaled.safetensors",
        "weight_dtype": "default"
      },
      "class_type": "UNETLoader",
      "_meta": {
        "title": "Load Flux Kria FP8 UNET"
      }
    },
    "2": {
      "inputs": {
        "clip_name1": "clip_l.safetensors",
        "clip_name2": "t5xxl_fp16.safetensors",
        "type": "flux"
      },
      "class_type": "DualCLIPLoader",
      "_meta": {
        "title": "Load CLIP L + T5XXL Text Encoders"
      }
    },
    "3": {
      "inputs": {
        "vae_name": "ae.safetensors"
      },
      "class_type": "VAELoader",
      "_meta": {
        "title": "Load VAE Encoder"
      }
    },
    "4": {
      "inputs": {
        "text": "stunning professional photography, sharp focus, cinematic lighting, highly detailed, vibrant colors, masterpiece, best quality, award-winning, intricate details, ultra high resolution",
        "clip": ["2", 0]
      },
      "class_type": "CLIPTextEncode",
      "_meta": {
        "title": "Positive Prompt"
      }
    },
    "5": {
      "inputs": {
        "text": "low quality, blurry, distorted, deformed, bad anatomy, watermark, text, logo, artifacting, compression",
        "clip": ["2", 0]
      },
      "class_type": "CLIPTextEncode",
      "_meta": {
        "title": "Negative Prompt"
      }
    },
    "6": {
      "inputs": {
        "model": ["1", 0],
        "clip": ["2", 0],
        "lora_stack": "[{\"lora\": \"FLUX.1-Turbo-Alpha.safetensors\", \"strength\": 0.7, \"strengthTwo\": 0.7, \"on\": true}, {\"lora\": \"facebookQuality.3t4R.safetensors\", \"strength\": 0.6, \"strengthTwo\": 0.6, \"on\": true}, {\"lora\": \"DetailTweaker_SDXL.safetensors\", \"strength\": 0.5, \"strengthTwo\": 0.5, \"on\": true}]"
      },
      "class_type": "MultiLoRAStack",
      "_meta": {
        "title": "Multi-LoRA Stack (3x LoRAs)"
      }
    },
    "7": {
      "inputs": {
        "width": 1024,
        "height": 1024,
        "batch_size": 1
      },
      "class_type": "EmptyLatentImage",
      "_meta": {
        "title": "Initialize Latent Space (1024x1024)"
      }
    },
    "8": {
      "inputs": {
        "seed": 0,
        "steps": 10,
        "cfg": 2.2,
        "sampler_name": "euler",
        "scheduler": "simple",
        "denoise": 1.0,
        "model": ["6", 0],
        "positive": ["4", 0],
        "negative": ["5", 0],
        "latent_image": ["7", 0]
      },
      "class_type": "KSampler",
      "_meta": {
        "title": "Turbo Sampler - 10 Steps, CFG 2.2"
      }
    },
    "9": {
      "inputs": {
        "samples": ["8", 0],
        "vae": ["3", 0]
      },
      "class_type": "VAEDecode",
      "_meta": {
        "title": "VAE Decode to Image"
      }
    },
    "10": {
      "inputs": {
        "filename_prefix": "multilora_turbo_",
        "images": ["9", 0]
      },
      "class_type": "SaveImage",
      "_meta": {
        "title": "Save Generated Image"
      }
    }
  }
}
